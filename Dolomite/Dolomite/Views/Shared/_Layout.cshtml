<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Dolomite - @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="row">
        <nav class="navbar navbar-fixed-top navbar-light">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#sidebar" aria-controls="sidebar" aria-expanded="false" aria-label="Toggle nuuuuuuuuu">
                <span class="navbar-toggler-icon"></span>
            </button>
        </nav>
    </div>
    <div class="row row-offcanvas row-offcanvas-right">
        <div class="collapse navbar-collapse row row-offcanvas row-offcanvas-left col-md-2" id="sidebar">
            <div class="nav flex-column nav-pills" role="tablist">
                <button type="button">test1</button>
                <button type="button">test2</button>
                <button type="button">test3</button>
                <button type="button">test4</button>
            </div>
        </div>
        <div class="col-md-10 whu">
            <div class="container body-content">
                @RenderBody()
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

